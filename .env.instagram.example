# Instagram Basic Display API Configuration
# Get these from https://developers.facebook.com/apps/
INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token_here
INSTAGRAM_APP_ID=your_instagram_app_id_here
INSTAGRAM_APP_SECRET=your_instagram_app_secret_here

# Note: Instagram Access Tokens
# - Short-lived tokens expire in 1 hour
# - Long-lived tokens expire in 60 days and need refreshing
# - Use the Instagram Basic Display API to get user media
# - For business accounts, consider Instagram Graph API for more features

# Development/Fallback Settings
INSTAGRAM_FALLBACK_ENABLED=true
INSTAGRAM_CACHE_DURATION=1800000  # 30 minutes in milliseconds